{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch simpleqt (gdb)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/simpleqt.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "PATH",
          "value": "D:/app/qt/Tools/CMake_64/bin;D:/app/qt/Tools/mingw1310_64/bin;D:/app/qt/6.10.1/mingw_64/bin;${env:PATH}"
        }
      ],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "D:/app/qt/Tools/mingw1310_64/bin/gdb.exe",
      "preLaunchTask": "Build (CMake)"
    }
  ]
}
